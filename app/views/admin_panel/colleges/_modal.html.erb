<div class="modal fade" id="college-modal-content" tabindex="-1" role="dialog" aria-labelledby="collegeModalLabel" aria-hidden="true">
  <div class="modal-dialog" role="document">
    <div class="modal-content">
      <div class="modal-header">
        <h5 class="modal-title" id="exampleModalLabel"><%= @college.persisted? ? "Edit" : "Create New" %> College</h5>
        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
          <span aria-hidden="true">&times;</span>
        </button>
      </div>
      <div class="college-modal-body">
        <%= render 'form', college: @college %>
      </div>
    </div>
  </div>
</div>
