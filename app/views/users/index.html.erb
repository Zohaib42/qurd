<div class="container mt-5">
  <div class="row">
    <div class="col">
      <h1 class="display-4 mb-4">Users</h1>
      <table class="table table-striped">
        <thead>
        <tr>
          <th scope="col">Email</th>
        </tr>
        </thead>
        <tbody>
          <% @users.each do |user| %>
            <tr>
              <td><%= user.email %></td>
            </tr>
          <% end %>
        </tbody>
      </table>
    </div>
  </div>
</div>